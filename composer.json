{
	"name": "anavaro/abannouncements",
	"type": "phpbb-extension",
	"description": "Adds extra featured board announcements",
	"homepage": "http://lab.anavaro.com/forum/viewforum.php?f=7",
	"version": "0.1.0",
	"time": "2017-07-17",
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Stanislav Atanasov",
			"email": "lucifer@anavaro.com",
			"homepage": "http://anavaro.com",
			"role": "Lead Developer"
		}
	],
	"require": {
		"php": ">=5.3"
	},
	"require-dev": {
		"phpbb/epv": "dev-master",
        "satooshi/php-coveralls": "~1.0"
	},
	"extra": {
		"display-name": "phpBB 3.2 Advanced Board Announcements",
		"version-check": {
			"host": "lab.anavaro.com",
			"directory": "/phpbb-ext/3.2.x",
			"filename": "abannouncements.json"
		},
		"soft-require": {
			"phpbb/phpbb": ">=3.2.0-RC2"
		}
	}
}
